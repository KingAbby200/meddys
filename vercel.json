{
  "buildCommand": "npm ci && npm run build",
  "outputDirectory": "",
  "installCommand": "npm ci",
  "framework": "vite",
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript" }
      ]
    },
    {
      "source": "/assets/(.*)\\.css",
      "headers": [
        { "key": "Content-Type", "value": "text/css" }
      ]
    }
  ]
}
